{
    "cSpell.words": [
        "backreference",
        "codetag",
        "Docstrings",
        "fstring",
        "metaclass",
        "Pantone",
        "readwrite",
        "typehint"
    ],
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        {
            "column": 0,
            "color": "#F0F8FF20"
