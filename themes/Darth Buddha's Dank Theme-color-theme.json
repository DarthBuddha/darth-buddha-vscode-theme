{
    "name": "Darth Buddha's Dank Theme",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": {
            "fontStyle": "italic bold",
            "foreground": "#00AB84"
        },
        "type": {
            "fontStyle": "bold",
            "foreground": "#00AB84"
        },
        "type.defaultLibrary": {
            "fontStyle": "strikethrough",
            "foreground": "#E4002B"
        },
        "struct": {
            "foreground": "#00AB84"
        },
        "class": {
            "fontStyle": "bold",
            "foreground": "#00AB84"
        },
        "class.defaultLibrary": {
            "fontStyle": "italic",
            "foreground": "#00AB84"
        },
        "interface": {
            "fontStyle": "strikethrough",
            "foreground": "#E4002B"
        },
        "enum": {
            "fontStyle": "bold",
            "foreground": "#00AB84"
        },
        "function": {
            "foreground": "#F0E87B"
        },
        "function.defaultLibrary": {
            "fontStyle": "bold",
            "foreground": "#F0E87B"
        },
        "method": {
            "foreground": "#F0E87B"
        },
        "macro": {
            "fontStyle": "strikethrough",
            "foreground": "#E4002B"
        },
        "variable": {
            "foreground": "#5BC2E7"
        },
        "variable.readonly": {
            "fontStyle": "bold",
            "foreground": "#5BC2E7"
        },
        "variable.readonly.defaultLibrary": {
            "fontStyle": "bold underline",
            "foreground": "#5BC2E7"
        },
        "parameter": {
            "fontStyle": "italic",
            "foreground": "#5BC2E7"
        },
        "property": {
            "fontStyle": "italic bold",
            "foreground": "#5BC2E7"
        },
        "property.readonly": {
            "fontStyle": "italic bold underline",
            "foreground": "#5BC2E7"
        },
        "enumMember": {
            "fontStyle": "italic",
            "foreground": "#5BC2E7"
        },
        "event": {
            "fontStyle": "strikethrough",
            "foreground": "#E4002B"
        },
    },
    "tokenColors": [
        {
            "name": "python",
            "scope": [
                "source.python",
            ],
            "settings": {
                "foreground": "#C7C9C7"
            }
        },
        {
            "name": "python-punctuation",
            "scope": [
                "punctuation.separator.period.python",
                "punctuation.separator.colon.python",
                "punctuation.separator.arguments.python",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C7C9C7"
            }
        },
        {
            "name": "python-operator",
            "scope": [
                "keyword.operator.assignment.python",
                "keyword.operator.comparison.python",
                "keyword.operator.logical.python",
                "keyword.operator.arithmetic.python",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E4002B"
            }
        },
        {
            "name": "Python - f String",
            "scope": [
                "storage.type.string.python",
                "constant.character.format.placeholder.other.python",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#005EB8"
            }
        },
        {
            "name": "Python - String Starts",
            "scope": [
                "punctuation.definition.string.begin.python",
                "punctuation.definition.string.end.python",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF8F1C"
            }
        },
        {
            "name": "Python - String",
            "scope": [
                "string.quoted.single.python",
                "string.quoted.docstring.multi.python",
            ],
            "settings": {
                "foreground": "#FDD086"
            }
        },
        {
            "name": "Python - Docstring",
            "scope": [
                "string.quoted.docstring.multi.python",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#707372"
            }
        },
        {
            "name": "python-comment",
            "scope": [
                "comment.line.number-sign.python",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#00B140"
            }
        },
        {
            "name": "python-keyword",
            "scope": [
                "keyword.control.import.python",
                "keyword.control.flow.python",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#963CBD"
            }
        },
        {
            "name": "python-meta",
            "scope": [
                "meta.function-call.python",
                "punctuation.definition.decorator.python",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#F0E87B"
            }
        },
        {
            "name": "python-storage",
            "scope": [
                "storage.type.function.python",
                "storage.type.function.async.python",
                "meta.class.python",
                "constant.language.python",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#005EB8"
            }
        },
        {
            "name": "python-constant",
            "scope": [
                "constant.other.caps.python",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#5BC2E7"
            }
        },
        {
            "name": "python-constant.numeric",
            "scope": [
                "constant.numeric.dec.python",
            ],
            "settings": {
                "foreground": "#6CC24A"
            }
        },
        {
            "name": "dotenv",
            "scope": [
                "source.dotenv",
            ],
            "settings": {
                "foreground": "#C7C9C7"
            }
        },
        {
            "name": "dotenv-comment",
            "scope": [
                "comment.line.dotenv",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#00B140"
            }
        },
        {
            "name": "dotenv-variable",
            "scope": [
                "variable.key.dotenv",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#5BC2E7"
            }
        },
        {
            "name": "dotenv-value",
            "scope": [
                "property.value.dotenv",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E4002B"
            }
        },
        {
            "name": "dotenv-keyword.operator",
            "scope": [
                "keyword.operator.assignment.dotenv",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C7C9C7"
            }
        },
        {
            "name": "gitignore-text",
            "scope": [
                "text.gitignore",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C7C9C7"
            }
        },
        {
            "name": "gitignore-comment",
            "scope": [
                "comment.gitignore",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#00B140"
            }
        },
        {
            "name": "markdown-heading",
            "scope": [
                "heading.1.markdown",
                "heading.2.markdown",
                "heading.3.markdown",
                "heading.4.markdown",
                "heading.5.markdown",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#3A913F"
            }
        },
        {
            "name": "markdown-list",
            "scope": [
                "punctuation.definition.list.begin.markdown",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#E4002B"
            }
        },
        {
            "name": "markdown-text",
            "scope": [
                "meta.paragraph.markdown",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#C7C9C7"
            }
        },
        {
            "name": "markdown-text",
            "scope": [
                "markup.bold.markdown",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#C7C9C7"
            }
        },
        {
            "name": "Markdown - Text Bold",
            "scope": [
                "punctuation.definition.bold.markdown",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#963821"
            }
        },
        {
            "name": "markdown-text",
            "scope": [
                "meta.link.inline.markdown",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#009CDE"
            }
        },
        {
            "name": "Json - text",
            "scope": [
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF8F1C"
            }
        },
        {
            "name": "Json - text",
            "scope": [
                "support.type.property-name.json",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FDD086"
            }
        },
        {
            "name": "Json - text",
            "scope": [
                "punctuation.separator.dictionary.key-value.json",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E4002B"
            }
        },
        {
            "name": "Json - text",
            "scope": [
                "punctuation.definition.string.begin.json",
                "punctuation.definition.string.end.json",
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#5BC2E7"
            }
        },
        {
            "name": "Json - text",
            "scope": [
                "string.quoted.double.json",
            ],
            "settings": {
                "fontStyle": "italic bold",
                "foreground": "#C7C9C7"
            }
        },
    ],
