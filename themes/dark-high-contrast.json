      "settings": {
        "fontStyle": "bold",
        "foreground": "#91cbff"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#72f088"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f0f3f6"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f0f3f6"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
