      "settings": {
        "foreground": "#addcff"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#72f088"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#91cbff"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#91cbff"
      }
    },
    {
